    <LocalDebuggerEnvironment>PATH=$(ProjectDir)..\dependencies\Qt\bin%3b"$(ProjectDir)..\dependencies\Qt\bin%3b$(PATH)</LocalDebuggerEnvironment>
    <LocalDebuggerCommand>$(TargetName)$(TargetExt)</LocalDebuggerCommand>
